[mysqld]
# Log General
general_log = 1
general_log_file = /tmp/mysql_general.log
# ndbcluster
# ndb-connectstring=localhost
# user=metabase

# Log Error
log_error = /tmp/mysql_error.log

# Log Slow
slow_query_log = 1
slow_query_log_file = /tmp/mysql_slow_query.log
long_query_time = 0  # 0 has the effect of logging all queries
log_queries_not_using_indexes = 1

# [mysql_cluster]
# ndb-connectstring=localhost